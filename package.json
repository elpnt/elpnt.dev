{
  "name": "elpnt.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"yarn generate --watch\"",
    "build": "next build",
    "start": "next start",
    "eslint": "next lint --dir src",
    "eslint:fix": "next lint --dir src --fix",
    "prettier": "prettier --check './src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --check './src/**/*.{ts,tsx}' --write",
    "lint": "yarn eslint && yarn prettier",
    "fix": "yarn eslint:fix && yarn prettier:fix",
    "generate": "graphql-codegen -r dotenv/config --config .graphqlrc"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "clsx": "^1.2.1",
    "graphql": "^16.6.0",
    "graphql-config": "^4.3.3",
    "graphql-request": "^4.3.0",
    "markdown-to-txt": "^2.0.1",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.11.6",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "18.7.9",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  }
}
